<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="library_product_view_form" model="ir.ui.view">
        <field name="name">library.product.view.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>

        <field name="arch" type="xml">
            <xpath expr="//form/header" position="inside">
                <button name="export_stock_level_button" string="Export Stock Level" type="object"/>
            </xpath>
        </field>
    </record>
</odoo>